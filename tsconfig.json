{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "*"
      ]
    }
  },
  "exclude": [
    "tmp",
    "slate"
  ]
}
